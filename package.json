    "aos": "^3.0.0-beta.6",
    "autoprefixer": "^9.7.4",
    "postcss-cli": "^7.1.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-icons": "^3.9.0",
